{
  "name": "ityukit-mdbbs",
  "version": "0.0.0",
  "description": "",
  "exports": "./src/bin/run.js",
  "type": "module",
  "scripts": {
    "createPGDB": "NODE_ENV=production node ./src/bin/createPGDB.js",
    "clearCache": "NODE_ENV=production node ./src/bin/clearCache.js",
    "build:migrate": "NODE_ENV=production ./node_modules/.bin/knex migrate:latest --env production --knexfile ./src/knex/migrate.js",
    "build:migrate:rollback": "NODE_ENV=production ./node_modules/.bin/knex migrate:rollback --env production --knexfile ./src/knex/migrate.js",
    "build:migrate:reset": "NODE_ENV=production ./node_modules/.bin/knex migrate:rollback --all --env production --knexfile ./src/knex/migrate.js",
    "build:migrate:up": "NODE_ENV=production ./node_modules/.bin/knex migrate:up --env production --knexfile ./src/knex/migrate.js",
    "build:migrate:down": "NODE_ENV=production ./node_modules/.bin/knex migrate:down --env production --knexfile ./src/knex/migrate.js",
    "build:seed:run": "NODE_ENV=production ./node_modules/.bin/knex seed:run --env production --knexfile ./src/knex/migrate.js",
    "build:db": "npm run build:migrate && npm run build:seed:run",
    "build:db:reset": "npm run build:migrate:reset && npm run build:migrate && npm run build:seed:run",
    "start": "NODE_ENV=production node ./src/bin/run.js",
    "userApply": "NODE_ENV=production node ./src/bin/userApply.js",
    "dev:createPGDB": "NODE_ENV=development node ./src/bin/createPGDB.js",
    "dev:clearCache": "NODE_ENV=development node ./src/bin/clearCache.js",
    "dev:build:migrate": "NODE_ENV=development ./node_modules/.bin/knex migrate:latest --env development --knexfile ./src/knex/migrate.js",
    "dev:build:migrate:rollback": "NODE_ENV=development ./node_modules/.bin/knex migrate:rollback --env development --knexfile ./src/knex/migrate.js",
    "dev:build:migrate:reset": "NODE_ENV=development ./node_modules/.bin/knex migrate:rollback --all --env development --knexfile ./src/knex/migrate.js",
    "dev:build:migrate:up": "NODE_ENV=development ./node_modules/.bin/knex migrate:up --env development --knexfile ./src/knex/migrate.js",
    "dev:build:migrate:down": "NODE_ENV=development ./node_modules/.bin/knex migrate:down --env development --knexfile ./src/knex/migrate.js",
    "dev:build:seed:run": "NODE_ENV=development ./node_modules/.bin/knex seed:run --env development --knexfile ./src/knex/migrate.js",
    "dev:build:db": "npm run dev:build:migrate && npm run dev:build:seed:run",
    "dev:build:db:reset": "npm run dev:build:migrate:reset && npm run dev:build:migrate && npm run dev:build:seed:run",
    "dev:start": "NODE_ENV=development node ./src/bin/run.js",
    "dev:userApply": "NODE_ENV=development node ./src/bin/userApply.js",
    "dev:sample": "npm run dev:build:db:reset && NODE_ENV=development node",
    "dev:debug:CleanRun": "npm run dev:createPGDB -- --user postgres --password postgres --rebuild && npm run dev:clearCache && npm run dev:build:db:reset && npm run dev:userApply -- --id admin --password admin --tier owner && npm run dev:start",
    "dev": "npm run dev:build:db && npm run dev:start"
  },
